6. Eliminar vocales
#include <iostream>
#include <string>
using namespace std;

int main(){
    string texto,resultado="";
    getline(cin,texto);

    for(char c:texto){
        if(!(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||
             c=='A'||c=='E'||c=='I'||c=='O'||c=='U'))
            resultado += c;
    }

    cout << resultado;
    return 0;
}
