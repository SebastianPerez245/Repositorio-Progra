10. Contar doble vocal (ae, ei, io...)
#include <iostream>
#include <string>
using namespace std;

bool vocal(char c){
    c=tolower(c);
    return c=='a'||c=='e'||c=='i'||c=='o'||c=='u';
}

int main(){
    string texto;
    getline(cin,texto);

    int contador=0;
    for(int i=0;i<texto.size()-1;i++)
        if(vocal(texto[i]) && vocal(texto[i+1]))
            contador++;

    cout << contador << " grupos de vocales";
    return 0;
}
